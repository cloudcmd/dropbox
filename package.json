    "squad": "^2.0.0",
    "string-to-stream": "^1.1.0",
    "through2": "^2.0.3",
    "try-to-catch": "^1.0.0",
    "zames": "^1.1.3"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "clear-module": "^2.1.0",
    "coveralls": "^3.0.0",
    "eslint": "^4.16.0",
    "fast-async": "^6.3.0",
    "mock-require": "^3.0.1",
    "nodemon": "^1.14.11",
